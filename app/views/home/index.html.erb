<div data-controller="attempt" class="text-center">
	<div class="text-center inline-block">
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-0" class="w-full font-semibold h-8 align-top border bg-emerald-100 py-1 my-1 ">Basic Rates</button>
		<br/>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-1" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">B1<br/>1x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-2" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">B2<br/>1.5x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-3" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">B3<br/>2x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-4" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">BP<br/>Perf</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-5" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">BPS<br/>Prem Perf</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-6" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">B1m<br/>1x MP</button>
		<br/>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-7" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">B2m<br/>1.5x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-8" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">B3m<br/>2x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-9" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">S2<br/>Snd 1.5x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-10" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">S3<br/>Snd 2x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-11" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">S2m<br/>Snd 1.5x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="basic-12" class="w-24 text-sm font-semibold h-12 align-top border bg-emerald-100 py-1 my-1">S3m<br/>Snd 2x MP</button>
	</div>
	<div class="text-center inline-block">
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-0" class="w-full font-semibold h-8 align-top border bg-amber-100 py-1 my-1 ">Other Rates</button>
		<br/>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-1" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">C1<br/>Cam 1x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-2" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">C2<br/>Cam 1.5x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-3" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">C3<br/>Cam 2x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-7" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">-<br/>Clear</button>
		<br/>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-4" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">C1m<br/>Cam 1x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-5" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">C2m<br/>Cam 1.5x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="other-6" class="w-24 text-sm font-semibold h-12 align-top border bg-amber-100 py-1 my-1">C3m<br/>Cam 2x MP</button>
		<button class="w-24 text-sm font-semibold h-12 align-top border py-1 my-1"></button>
	</div>

	<div class="text-center inline-block">
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-0" class="w-full font-semibold h-8 align-top border bg-lime-100 py-1 my-1 ">Extra Rates</button>
		<br/>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-1" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E1<br/>1x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-2" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E2<br/>1.5x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-3" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E3<br/>2x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-4" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E4<br/>Sun 1x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-5" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E5<br/>Sun 1.5x</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-6" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">EP<br/>Perf</button>
		<br/>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-7" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E1m<br/>1x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-8" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E2m<br/>1.5x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-9" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E3m<br/>2x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-10" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E4m<br/>Sun 1x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-11" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">E5m<br/>Sun 1.5x MP</button>
		<button data-action="click->attempt#select_rate_group" data-attempt-target="rateIndicator" id="extra-12" class="w-24 text-sm font-semibold h-12 align-top border bg-lime-100 py-1 my-1">EPS<br/>Prem Perf</button>
	</div>
	<div class="text-center inline-block">
		<button class="w-full font-semibold h-8 align-top border bg-purple-100 py-1 my-1 ">Hour Quantity</button>
		<br/>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-1" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">1</button>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-2" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">2</button>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-3" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">3</button>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-4" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">4</button>
		<br/>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-5" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">5</button>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-6" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">6</button>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-7" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">7</button>
		<button data-action="click->attempt#select_hour_quantity_from_button" data-attempt-target="hourQuantityIndicator" id="select-hour-quantity-8" class="w-24 text-sm font-semibold h-12 border align-top bg-purple-100 my-1 py-1">8</button>
	</div>
	<br/>
	<button data-action="click->attempt#clear_values" class="w-32 border align-top bg-red-300 my-2 py-1">Clear Values</button>
	<button data-action="click->attempt#ath_shifts" class="w-32 border align-top bg-green-300 my-2 py-1">ATH Shifts</button>
	<button data-action="click->attempt#dgh_shifts" class="w-32 border align-top bg-green-300 my-2 py-1">DGH Shifts</button>
	<button data-action="click->attempt#generate_json" class="w-32 border align-top bg-orange-300 my-2 py-1">Generate JSON</button>
	<button data-action="click->attempt#read_json" class="w-32 border align-top bg-yellow-300 my-2 py-1">Read JSON</button>
	<br/>

	<div class="bg-slate-300 text-left w-full p-0 mx-0 my-0 p-1">
		<div class="block">

			<button class="bg-gray-100 text-center w-6 h-8 inline-block text-sm p-0 m-0">
				<%= icon( 'fa-solid', 'chevron-down' )%>
			</button>

			<button class="bg-gray-100 text-center w-48 h-8 inline-block text-sm p-0 m-0">
				Name
			</button>

			<% time = Time.now.midnight %>
			<% 0.upto(27) do |add| %>
  			<% if [12,13].include? add %>
					<button class="bg-yellow-300 text-center w-8 h-8 inline-block align-top text-sm font-semibold p-0 m-0"><%= (time + add.hours).strftime('%l%P').chop %></button>
				<% elsif [17,18].include? add %>
					<button class="bg-sky-300 text-center w-8 h-8 inline-block align-top text-sm font-semibold p-0 m-0"><%= (time + add.hours).strftime('%l%P').chop %></button>
				<% elsif [5].include? add %>
					<button class="bg-violet-300 text-center w-8 h-8 inline-block align-top text-sm font-semibold p-0 m-0"><%= (time + add.hours).strftime('%l%P').chop %></button>
				<% elsif add > 7 and add < 16 %>
					<button class="bg-yellow-100 text-center w-8 h-8 inline-block align-top text-sm font-semibold p-0 m-0"><%= (time + add.hours).strftime('%l%P').chop %></button>
				<% elsif add > 15 and add < 24 %>
					<button class="bg-sky-200 text-center w-8 h-8 inline-block align-top text-sm font-semibold p-0 m-0"><%= (time + add.hours).strftime('%l%P').chop %></button>
				<% else %>
					<button class="bg-violet-100 text-center w-8 h-8 inline-block align-top text-sm font-semibold p-0 m-0"><%= (time + add.hours).strftime('%l%P').chop %></button>
				<% end %>
			<% end %>
		</div>

		<% 10.times do |row| %>
			<div class="block px-0 pt-1">
				<button class="bg-gray-100 text-center w-6 h-8 inline-block text-sm m-0 p-0" title="Copy Employee Name" data-action="click->attempt#copy_employee_down" id="row-<%=row%>">
					<%= icon( 'fa-solid', 'chevron-down' )%>
				</button>
				<%= collection_select( :object, :method, Employee.active.order(:classification).order(:last_name).order(:first_name), :id, :full_name, {prompt: true}, {id: "row-#{row}", class: "bg-gray-100 align-top border-0 h-8 w-48 inline-block text-xs p-0 m-0", data: {attempt_target: "employee"}})%>

				<% time = Time.now.midnight %>
				<% 0.upto(27) do |hour| %>
					<button class="bg-gray-100 text-center w-8 h-8 inline-block text-sm p-0 m-0" title="" data-action="click->attempt#hourClicked" data-attempt-target="hour" id="row-<%=row%>-hour-<%=hour%>">-</button>
				<% end %>
				<button class="bg-gray-100 text-center w-6 h-8 inline-block text-sm p-0 m-0" title="Copy Rates Down" data-action="click->attempt#copy_hours_down" id="row-<%=row%>">
					<%= icon( 'fa-solid', 'turn-down' )%>
				</button>
			</div>
		<% end %>

	</div>
	
	<div class="bg-slate-100 text-left text-sm mt-6">
		JSON Output:<br/>
		<textarea id="json_output" data-attempt-target="json_output" rows="12" class="w-full">Foo</textarea>
	</div>
</div>
